{
  "name": "@hypewatch/hype-sdk",
  "version": "1.0.0",
  "description": "SDK for interacting with the Hype Protocol on Solana blockchain",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "biome check .",
    "format": "biome format . --write",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "playground": "tsx playground.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hypewatch/hype-sdk.git"
  },
  "keywords": [
    "solana",
    "blockchain",
    "sdk",
    "hype",
    "trading",
    "defi",
    "web3"
  ],
  "author": "Hype Protocol",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hypewatch/hype-sdk/issues"
  },
  "homepage": "https://github.com/hypewatch/hype-sdk#readme",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@solana/spl-token": "^0.4.9",
    "@solana/web3.js": "^1.98.0",
    "bignumber.js": "^9.1.2",
    "bs58": "^6.0.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.17.15",
    "@types/node": "^20.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.8.3"
  }
}